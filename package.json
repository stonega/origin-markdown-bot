{
  "name": "niagara-launcher-bot",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Instantly access and launch your favorite apps",
  "imports": {
    "#root/*": "./build/src/*"
  },
  "author": "stone <xijieyin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc",
    "dev": "bun --watch ./src/index.ts",
    "start": "tsc && bun ./src/index.ts",
    "sync": "bun run scripts/sync_apps.ts",
    "fmt": "bunx @biomejs/biome check --write */**",
    "prisma:migrate": "bunx prisma migrate dev",
    "start:force": "bun ./src/index.ts",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@grammyjs/auto-chat-action": "0.1.1",
    "@grammyjs/conversations": "^1.2.0",
    "@grammyjs/hydrate": "1.4.1",
    "@grammyjs/i18n": "1.0.2",
    "@grammyjs/menu": "^1.2.2",
    "@grammyjs/parse-mode": "1.10.0",
    "@grammyjs/runner": "2.0.3",
    "@grammyjs/storage-prisma": "^2.4.2",
    "@grammyjs/types": "3.13.0",
    "@hono/node-server": "1.13.0",
    "@prisma/client": "5.18.0",
    "@telegraf/entity": "^0.5.0",
    "callback-data": "1.1.1",
    "grammy": "1.29.0",
    "grammy-guard": "0.5.0",
    "hono": "4.5.8",
    "iso-639-1": "3.1.2",
    "pino": "9.3.2",
    "pino-pretty": "11.2.2",
    "valibot": "0.38.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.2",
    "@types/bun": "latest",
    "husky": "9.1.5",
    "lint-staged": "15.2.9",
    "prisma": "^5.17.0"
  },
  "lint-staged": {
    "*.ts": "bunx @biomejs/biome check"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
